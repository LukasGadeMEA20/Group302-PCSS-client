<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.yakboksdemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ToggleButton mnemonicParsing="false" text="ToggleButton" />
    <TextField id="input" fx:id="input" layoutX="120.0" layoutY="64.0" onAction="#onHelloButtonClick" promptText="Type somthing..." />

    <Label id="output" fx:id="output" text="This will be output if there is any..." />
    <CheckBox mnemonicParsing="false" text="CheckBox" />
    <Button onAction="#onHelloButtonClick2" text="Go to scene 1" />
</VBox>
